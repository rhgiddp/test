    <!-- 전체 로그 관련 -->
    <root>
      <level value="ALL" />
      <appender-ref ref="ConvertFileAppender" />
    </root>
    <logger name="Default">
      <level value="INFO" />
      <appender-ref ref="DefaultFileAppender" />
    </logger>
    <logger name="Panser">
      <level value="ALL" />
