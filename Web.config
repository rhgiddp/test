  <log4net>
    <appender name="ConvertFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="Logs/log.txt" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <maxSizeRollBackups value="0" />
      <maximumFileSize value="1MB" />
      <staticLogFileName value="true" />
      <datePattern value="_yyyyMMddHH&quot;.log&quot;" />
      <layout type="log4net.Layout.PatternLayout">%date [%thread] %-5level - %message%newline</layout>
    </appender>
    <appender name="ResultFileApeender" type="log4net.Appender.RollingFileAppender">
